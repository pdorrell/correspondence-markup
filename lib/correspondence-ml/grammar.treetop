grammar CorrespondenceMarkupLanguage

  rule structure_group
    "[" [\s]* (structure [\s]*)* "]"
  end

  rule structure
    "[" item_or_non_item* "]"
  end

  rule item_or_non_item
    item / non_item
  end

  rule non_item
    (![\[\]] .)+
  end

  rule item
    "[" number [\s]+ (![\]] .)* "]"
  end

  rule number
    [0-9]+
  end
end
